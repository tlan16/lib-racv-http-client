{
  "name": "lib-racv-http-client",
  "version": "0.0.3",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tlan16/lib-racv-http-client.git",
    "directory": "packages/lib"
  },
  "scripts": {
    "publish": "pnpm publish --access public",
    "publish:dry-run": "pnpm publish --access public --dry-run",
    "build": "tsup",
    "test": "node --no-warnings=ExperimentalWarning --loader @swc-node/register/esm --test **/*.spec.ts",
    "lint": "./node_modules/.bin/eslint",
    "check-types": "./node_modules/.bin/tsc --noEmit"
  },
  "main": "packages/lib/main.ts",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@stylistic/eslint-plugin-js": "^2.1.0",
    "@stylistic/eslint-plugin-ts": "^2.1.0",
    "@swc-node/register": "^1.9.1",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.12",
    "eslint": "^9.3.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0",
    "@types/user-agents": "^1.0.4",
    "@types/crypto-js": "^4.2.2",
    "tsup": "^8.1.0"
  },
  "dependencies": {
    "zod": "^3.23.8",
    "crypto-js": "^4.2.0",
    "user-agents": "^1.1.216"
  }
}
